{
  "name": "acamica-dwfs-belgrano",
  "version": "11.0.0",
  "description": "acamica-dwfs-belgrano",
  "scripts": {
    "clean-deploys": "now rm $INSTANCE_NAME --token $NOW_TOKEN --yes",
    "deploy": "now alias $(now --name $INSTANCE_NAME -e NODE_ENV=production --npm --token $NOW_TOKEN) $INSTANCE_NAME --token=$NOW_TOKEN",
    "dev": "npm run fetch-dataset && npm run start",
    "fetch-dataset": "env-cmd .env node scripts/fetch-dataset",
    "scale": "now scale $INSTANCE_NAME 1 --token $NOW_TOKEN",
    "start": "serve public --single"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/durancristhian/acamica-dwfs-belgrano.git"
  },
  "author": "@durancristhian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/durancristhian/acamica-dwfs-belgrano/issues"
  },
  "homepage": "https://github.com/durancristhian/acamica-dwfs-belgrano#readme",
  "dependencies": {
    "serve": "^9.0.0"
  },
  "devDependencies": {
    "env-cmd": "^8.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "gsheets": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "prettier": "^1.13.5",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "contributors": [
    {
      "name": "Cristhian Duran",
